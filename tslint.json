{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "jsx-no-multiline-js":false,
    "jsx-curly-spacing":true,
    "object-literal-sort-keys": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "quotemark": [true, "single", "jsx-double"],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator",
      "check-preblock"
    ]
  }
}